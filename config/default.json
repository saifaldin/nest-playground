{
  "port": 3000,
  "db": {
    "type": "postgres",
    "host": "localhost",
    "port": 5432,
    "entities": ["dist/**/*.entity.{ts,js}"],
    "synchronize": true
  },
  "authentication": {
    "jwt": {
      "expiresIn": "3600s"
    }
  },
  "logging": {
    "log_rotation_limit": 10000000,
    "log_blacklisted_meta_fields": ["trace"]
  }
}
